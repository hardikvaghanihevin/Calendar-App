<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomView">
        <attr name="running_year" format="integer" />
        <attr name="running_month" format="integer" />
        <attr name="running_date" format="integer" />

        <attr name="month_name_with_year" format="boolean" />

        <attr name="text_size_month" format="dimension" />
        <attr name="text_size_day" format="dimension" />
        <attr name="text_size_date" format="dimension" />

        <attr name="text_color_month" format="color"/>
        <attr name="text_color_day" format="color"/>
        <attr name="text_color_date" format="color"/>

        <attr name="background_color_month" format="color"/>
        <attr name="background_color_day" format="color"/>
        <attr name="background_color_date" format="color"/>

        <attr name="background_drawable_month" format="reference"/>
        <attr name="background_drawable_day" format="reference"/>
        <attr name="background_drawable_date" format="reference"/>

        <attr name="week_start" format="enum">
            <enum name="sunday" value="0" />
            <enum name="monday" value="1" />
        </attr>
        <attr name="month_display_option" format="enum">
            <enum name="hideAdjacentMonthDates" value="0" />
            <enum name="showPreviousMonthDates" value="1" />
            <enum name="showNextMonthDates" value="2" />
            <enum name="showAdjacentMonthDates" value="3" />
        </attr>

        <attr name="iconDrawable" format="reference" />
        <attr name="iconBackArrow" format="reference" />

        <attr name="iconYear" format="reference" />
        <attr name="iconMonth" format="reference" />
        <attr name="iconCountry" format="reference" />
        <attr name="iconLanguage" format="reference" />
        <attr name="iconCalendar" format="reference" />
        <attr name="iconJumpToDate" format="reference" />
        <attr name="iconTheme" format="reference" />
        <attr name="iconRateApp" format="reference" />
        <attr name="iconPrivacy" format="reference" />
        <attr name="iconDeviceInfo" format="reference" />



    </declare-styleable>
</resources>